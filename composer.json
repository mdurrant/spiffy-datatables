{
    "name": "spiffy/spiffy-datatables",
    "description": "ZF2 module for programmatic creation of jQuery Datatables.",
    "homepage": "http://www.github.com/spiffyjr/spiffy-datatables.git",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-json": "2.*",
        "zendframework/zend-view": "2.*"
    },
    "require-dev": {
        "mockery/mockery": "0.*",
        "phpunit/phpunit": "3.*",
        "spiffy/spiffy-test": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "SpiffyDatatables": "src/",
            "SpiffyDatatablesTest": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}

